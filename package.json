{
  "name": "forte-engenharia",
  "version": "0.0.1",
  "description": "Forte Engenharia Website",
  "author": "Douglas Junior <douglasrtl45@gmail.com>",
  "private": false,
  "keywords": [
    "11ty",
    "SASS",
    "SCSS",
    "eleventy"
  ],
  "license": "ISC",
  "scripts": {
    "watch:sass": "node-sass --watch src/sass -o src/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "node-sass src/sass -o src/css",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass --parallel build:*"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "axios": "^1.6.7",
    "eleventy-plugin-error-overlay": "^1.0.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "showdown": "^2.1.0",
    "yarn": "^1.22.19"
  }
}
